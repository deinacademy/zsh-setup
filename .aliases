#!/bin/bash

# Utils
add_source ~/.dotfiles/alias/terminal
add_source ~/.dotfiles/alias/utils

# Development
add_source ~/.dotfiles/alias/android
add_source ~/.dotfiles/alias/code
add_source ~/.dotfiles/alias/fzf
add_source ~/.dotfiles/alias/git
add_source ~/.dotfiles/alias/swpm
add_source ~/.dotfiles/alias/swpx
add_source ~/.dotfiles/alias/volta
add_source ~/.dotfiles/alias/python
add_source ~/.dotfiles/alias/zinit
add_source ~/.dotfiles/alias/zmk

# Files and Folders
add_source ~/.dotfiles/alias/files

# Updates
if [[ "$( which_os )" == "OSX" ]]; then
    add_source ~/.dotfiles/alias/macos
fi
if [[ "$( which_os )" == "LINUX" ]]; then
    add_source ~/.dotfiles/alias/unix
    add_source ~/.dotfiles/alias/ubuntu
fi

# Private
add_source ~/.dotfiles/alias/private
