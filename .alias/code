#!/bin/bash

# VSCode
alias cc="code --profile \"Default\""
alias vco="code --profile \"Default\" ."
alias vcn="code --profile \"Default\" . -n"
alias vd="code --profile \"DotNet\""
alias vdo="code --profile \"DotNet\" ."
alias vr="code --profile \"React\""
alias vro="code --profile \"React\" ."
alias vn="code --profile \"Node\""
alias vno="code --profile \"Node\" ."
alias vw="code --profile \"WSL\" --remote wsl+Ubuntu --goto"
alias vwo="code --profile \"WSL\" --remote wsl+Ubuntu --goto "$PWD"/"



# Extensions
# npm install -g vsce
alias vcp="vsce package --out build/"