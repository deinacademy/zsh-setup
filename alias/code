#!/bin/bash

# VSCode
alias vc="code --profile Default" # <path|file>
alias vco=".open_code -p Default"
alias vd="code --profile DotNet" # <path|file>
alias vdo=".open_code -p DotNet"
alias vr="code --profile React" # <path|file>
alias vro=".open_code -p React"
alias vn="code --profile Node" # <path|file>
alias vno=".open_code -p Node"
alias vb="code --profile Bun" # <path|file>
alias vbo=".open_code -p Bun"

# Extensions
# sag vsce
alias vcp="vsce package --out build/"