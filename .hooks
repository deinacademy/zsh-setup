#!/bin/bash

# Hooks
local quit="y"
local cmd=""
local cmd_alias=""

# File
local file="package.json"

[ -f ~/.dotfiles/hooks/pos_validation ] && . ~/.dotfiles/hooks/pos_validation
[ -f ~/.dotfiles/hooks/pre_validation ] && . ~/.dotfiles/hooks/pre_validation

autoload -U add-zsh-hook                  # Load the zsh hook module
